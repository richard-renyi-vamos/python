# MODULE IMPORT
import matplotlib.pyplot as plt

# AXIS VALUES
x = [500,30,70]

# Y AXIS VALUES
y = [10,30,70]

# POINT PLOTTING
plt.plot(x, y)

# X AXIS NAMING
plt.xlabel('x - axis')
# naming the y axis
plt.ylabel('y - axis')

# GRAPH TITLE
plt.title('This is an awesome graph! â˜º')

# SHOW THE PLOT!
plt.show()
